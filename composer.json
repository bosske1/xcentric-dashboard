{
    "name": "xcentric/dashboard",
    "description": "All the basic components for a dashboard based on symfony 4",
    "type": "symfony-bundle",
    "keywords": ["Symfony", "bundle", "dashboard"],
    "homepage": "https://github.com/bosske1/xcentric-dashboard",
    "license": "MIT",
    "require": {
        "php": "^7.1",
		"doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "gedmo/doctrine-extensions": "^2.4",
        "jms/serializer-bundle": "^2.3",
        "symfony/framework-bundle": "^4.0",
		"symfony/flex": "^1.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/orm-pack": "^1.0",
		"symfony/yaml": "^4.0"
    },
	"autoload": {
        "psr-4": { "Xcentric\\Bundle\\XcentricDashboardBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
